package AOP;

import org.springframework.stereotype.Component;

@Component // 스프링 빈으로 등록
public class Calc {

    public int Add(int x, int y) {
        // 순수 주업무만 남김
        int result = x + y;
        return result;
    }

    public int Mul(int x, int y) {
        int result = x * y;
        return result;
    }
}
